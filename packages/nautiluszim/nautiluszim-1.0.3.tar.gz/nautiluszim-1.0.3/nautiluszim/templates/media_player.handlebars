{{#each doc.items}}
  <p><a href="{{this.fpath}}"><img class="icon" src="{{this.icon}}" /> {{this.fname}}</a>
  {{#if this.is_file}}{{else}}
  <{{kind}} class="video-js vjs-default-skin nautilus-{{kind}}">
  <source src="{{this.fpath}}" type="{{this.mime}}" /></{{kind}}>
  {{/if}}
</p>
{{/each}}
<script>$('.video-js').each(function(){ videojs($(this)[0], videojs_options)});</script>
